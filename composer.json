{
    "name": "sonsofphp/clock",
    "type": "library",
    "description": "Provides a standard way of accessing datetime functions",
    "keywords": [
        "clock",
        "datetime",
        "date",
        "time",
        "psr20",
        "psr-20",
        "clock-interface",
        "iso-8601",
        "iso8601"
    ],
    "homepage": "https://github.com/SonsOfPHP/clock",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Estes",
            "email": "joshua@sonsofphp.com"
        }
    ],
    "support": {
        "issues": "https://github.com/SonsOfPHP/sonsofphp/issues",
        "forum": "https://github.com/orgs/SonsOfPHP/discussions"
    },
    "autoload": {
        "psr-4": {
            "SonsOfPHP\\Component\\Clock\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "provide": {
        "psr/clock-implementation": "1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4"
    },
    "extra": {
        "sort-packages": true,
        "branch-alias": {
            "dev-main": "0.2.x-dev"
        }
    }
}